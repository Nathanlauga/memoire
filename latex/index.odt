     <text:p><text:s/>\begin{equation}</text:p>
     <text:p>1/2 [|FPR_{D = unprivileged} - FPR_{D = privileged}| + |TPR_{D = unprivileged} - TPR_{D = privileged}|]</text:p>
     <text:p>\end{equation}</text:p>
    </office:annotation></text:p>
