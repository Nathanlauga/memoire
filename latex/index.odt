   <text:p text:style-name="Text_20_body">Le modèle d&apos;explication par exemple <draw:frame draw:style-name="fr1" draw:name="Image40" text:anchor-type="as-char" svg:width="1.997cm" svg:height="0.998cm" draw:z-index="0"><draw:image xlink:href="new_memoire/img19.svg" xlink:type="simple" xlink:show="embed" xlink:actuate="onLoad" loext:mime-type="image/svg+xml"/>
