      <text:p text:style-name="Table_20_Contents"><draw:frame draw:style-name="fr1" draw:name="Image9" text:anchor-type="as-char" svg:width="52.97cm" svg:height="15.796cm" draw:z-index="0"><draw:image xlink:href="new_memoire/pipeline_ml_project.png" xlink:type="simple" xlink:show="embed" xlink:actuate="onLoad" loext:mime-type="image/png"/>
